package org.egonet.web.page;

import org.apache.wicket.markup.html.link.Link;

public class Index extends EgonetPage
{
    public Index()
    {
    	super("Egonet");
        add(
        	new Link("authoringLink") {
        		public void onClick() {
        			setResponsePage(new Authoring());
        		}
        	});
    }
    
}


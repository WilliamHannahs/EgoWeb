<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="net.sf.egonet.model.Question" table="question">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="title" column="title" />
		<property name="prompt" column="prompt" />
		<property name="citation" column="citation" />
		<property name="subjectTypeDB" column="subject_type" />
		<property name="responseTypeDB" column="response_type" />
	</class>
</hibernate-mapping>



